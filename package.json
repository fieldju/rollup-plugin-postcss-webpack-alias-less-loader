{
  "name": "rollup-plugin-postcss-webpack-alias-less-loader",
  "version": "1.0.0",
  "description": "A custom less loader for rollup-plugin-postcss that supports webpack style aliases.",
  "main": "lib/index.js",
  "repository": "https://github.com/fieldju/rollup-plugin-postcss-webpack-alias-less-loader.git",
  "author": "Justin Field",
  "license": "Apache 2.0",
  "private": false,
  "scripts": {
    "jest": "jest test/ --coverage --collectCoverageFrom=lib/**/*.js",
    "clean": "rm -fr build",
    "format": "prettier --write 'lib/**/*.{ts,js}'",
    "lint": "eslint lib/**/*.js",
    "test": "yarn lint && yarn jest",
    "test-server": "node --inspect user-acceptance-test/server.js  | ./node_modules/.bin/bunyan",
    "publish-js-doc": "scripts/generate-jsdoc-site.sh"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "import-cwd": "^3.0.0",
    "less": "^3.9.0",
    "pify": "^4.0.1"
  }
}
